[user]
  name = Kevin Bao
  email = kzb@kevinbao.com
[core]
  editor = emacs -nw
  excludesfile = ~/.gitignore
[branch]
  autosetupmerge = always
[fetch]
  prune = true
[merge]
  ff = only
[pager]
  branch = false
[pull]
  rebase = true
[push]
  default = current
[remote]
  pushDefault = origin
[alias]
  a  = add
  b  = branch
  bb = checkout
  bc = checkout -b
  bk = branch -D
  cc = commit
  ca = commit --amend
  ce = commit --amend --no-edit
  cp = cherry-pick
  cu = reset HEAD^ --soft
  d  = diff
  dc = diff --cached
  dh = diff HEAD
  dl = diff HEAD^ HEAD
  f  = fetch origin
  ff = pull
  h  = log --pretty=format:\"%Cgreen%ad %Cred%h %Creset| %s %Cblue%an\" --date=short
  k  = checkout --
  ms = merge --squash
  pp = push
  pf = push -f
  ra = rebase --abort
  rb = rebase
  rc = rebase --continue
  ri = rebase --interactive
  s  = status
  u = reset --
  xc = reset HEAD^ --hard
  xx = reset HEAD --hard
  z  = stash
  zp = stash pop
  ignore = update-index --assume-unchanged
  unignore = update-index --no-assume-unchanged
