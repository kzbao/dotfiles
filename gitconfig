[user]
	name = Kevin Bao
	email = kzbao93@gmail.com
[core]
	editor = emacs -nw
	excludesfile = ~/.gitignore
[branch]
	autosetupmerge = always
[fetch]
        prune = true
[pager]
	branch = false
[pull]
        rebase = true
[push]
	default = current
[alias]
        a = add
        b = branch
        bb = checkout
        bc = checkout -b
        bk = branch -D
        cc = commit
        ca = commit --amend
        ce = commit --amend --no-edit
        cu = reset HEAD^ --soft
        d = diff
	dh = diff HEAD
        f = fetch origin
        ff = pull
        h = log --pretty=format:\"%Cgreen%ad %Cred%h %Creset| %s %Cblue%an\" --date=short
        ignore = update-index --assume-unchanged
        k = checkout --
	kh = reset HEAD
	kx = reset HEAD --hard
        l = diff HEAD^ HEAD
        ms = merge --squash
        pp = push
        pf = push -f
        pu = push -u origin
        rb = rebase
        rc = rebase --continue
        ri = rebase --interactive
        s = status
        unignore = update-index --no-assume-unchanged
        z = stash
        zp = stash pop
